<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>check_login</servlet-name>
        <servlet-class>check_login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>do_register</servlet-name>
        <servlet-class>do_register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_playlist</servlet-name>
        <servlet-class>add_playlist</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_song</servlet-name>
        <servlet-class>add_song</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>change_password</servlet-name>
        <servlet-class>change_password</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>change_mobile</servlet-name>
        <servlet-class>change_mobile</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>check_login</servlet-name>
        <url-pattern>/check_login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>do_register</servlet-name>
        <url-pattern>/do_register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_playlist</servlet-name>
        <url-pattern>/add_playlist</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_song</servlet-name>
        <url-pattern>/add_song</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>change_password</servlet-name>
        <url-pattern>/change_password</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>change_mobile</servlet-name>
        <url-pattern>/change_mobile</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
